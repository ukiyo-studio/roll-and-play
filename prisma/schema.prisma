generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Game {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  played    Boolean  @default(false)
  tier      String?
  tierOrder Int?
  createdAt DateTime @default(now())
}
